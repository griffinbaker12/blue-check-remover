{
  "manifest_version": 3,
  "name": "Blue check remover",
  "description": "Removes blue check marks from X accounts.",
  "permissions": ["activeTab"],
  "version": "1.0",
  "content_scripts": [
      {
          "matches": ["https://twitter.com/*", "https://x.com/*"],
          "js": ["content.js"]
      }
  ],
  "icons": {
      "48": "icon.png"
  }
}
